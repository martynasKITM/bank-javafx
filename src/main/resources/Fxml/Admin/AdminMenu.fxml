<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox prefHeight="700.0" prefWidth="250.0" styleClass="main_menu_container" stylesheets="@../../Styles/ClientMenu.css"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.benedict.minibank.Controllers.Admin.AdminMenuController">
    <AnchorPane prefHeight="700.0" prefWidth="160.0" styleClass="left_container">
        <VBox.margin>
            <Insets bottom="5.0"/>
        </VBox.margin>
        <VBox layoutX="14.0" layoutY="14.0" prefHeight="80.0" prefWidth="160.0" styleClass="title_container"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Maze Bank"/>
        </VBox>
        <VBox layoutX="60.0" layoutY="196.0" prefHeight="350.0" prefWidth="160.0" styleClass="navigation_container"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
            <Button fx:id="create_client_btn" mnemonicParsing="false" text="Create Client">
            </Button>
            <Button fx:id="clients_btn" mnemonicParsing="false" text="Clients">
            </Button>
            <Button fx:id="deposit_btn" mnemonicParsing="false" text="Deposit">
            </Button>
            <Button fx:id="logout_btn" mnemonicParsing="false" styleClass="alt_navigation_btn" text="Logout">
            </Button>
        </VBox>
    </AnchorPane>
</VBox>
